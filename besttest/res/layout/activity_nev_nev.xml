                    android:layout_height="wrap_content"
                    android:text=">" />
            </TableRow>

            <TableRow
                android:id="@+id/NevNevtableRow722"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="right" >
            </TableRow>
        </TableLayout>
    </TableRow>

</TableLayout>